{
    "name": "@w3gallery/openalea",
    "description": "Short showcase of openalea in Visual Studio Flow.",
    "version": "0.1.0",
    "author": "reinisch.gui@gmail.com",
    "license": "MIT",
    "homepage": "https://github.com/w3gallery/openalea#README.md",
    "main": "./dist/@w3gallery/openalea.js",
    "files": [
        "dist",
        "assets"
    ],
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:prod",
        "pre-build": "yarn clean",
        "post-build": "del-cli dist/src/tests",
        "build:dev": "yarn pre-build && webpack --mode development && yarn post-build",
        "build:prod": "yarn pre-build && webpack --mode production && yarn post-build",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "typedoc",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public"
    },
    "exports": {
        ".": {
            "default": "./dist/@w3gallery/openalea.js",
            "types": "./dist/src/lib/index.d.ts",
            "webpm": {
                "dependencies": [
                    "@w3nest/webpm-client",
                    "mkdocs-ts",
                    "@mkdocs-ts/notebook",
                    "rxjs",
                    "rx-vdom",
                    "@vs-flow/core",
                    "@vs-flow/flowchart-3d",
                    "@vs-flow/tb-openalea/Doc"
                ],
                "aliases": []
            }
        }
    },
    "dependencies": {
        "@mkdocs-ts/notebook": "^0.1.5",
        "@vs-flow/core": "^0.4.0",
        "@vs-flow/flowchart-3d": "^0.4.0",
        "@vs-flow/tb-openalea": "^0.1.0",
        "@w3nest/webpm-client": "^0.1.12",
        "mkdocs-ts": "^0.5.3",
        "rx-vdom": "^0.1.8",
        "rxjs": "^7.8.2"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/node": "^24.3.0",
        "@types/webpack": "^5.28.5",
        "del-cli": "^6.0.0",
        "eslint": "9.34.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jest": "^29.0.1",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^30.1.2",
        "jest-environment-jsdom": "^30.1.2",
        "jest-junit": "^16.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-properties": "^0.3.0",
        "ts-jest": "^29.4.1",
        "ts-loader": "9.5.4",
        "ts-node": "10.9.2",
        "tsx": "^4.20.5",
        "typedoc": "^0.28.12",
        "typescript": "5.9.2",
        "typescript-eslint": "8.41.0",
        "webpack": "^5.101.3",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "6.0.1"
    },
    "webpm": {
        "kind": "esm",
        "dependencies": {
            "@mkdocs-ts/notebook": "^0.1.5",
            "@vs-flow/core": "^0.4.0",
            "@vs-flow/flowchart-3d": "^0.4.0",
            "@vs-flow/tb-openalea": "^0.1.0",
            "@w3nest/webpm-client": "^0.1.12",
            "mkdocs-ts": "^0.5.3",
            "rx-vdom": "^0.1.8",
            "rxjs": "^7.8.2"
        }
    },
    "webpack": {
        "root": "src",
        "apiVersion": "01",
        "main": "./lib/index.ts",
        "additionalEntries": {},
        "externals": {
            "@mkdocs-ts/notebook": [
                "@mkdocs-ts/notebook_APIv01"
            ],
            "@vs-flow/core": [
                "@vs-flow/core_APIv04"
            ],
            "@vs-flow/flowchart-3d": [
                "@vs-flow/flowchart-3d_APIv04"
            ],
            "@vs-flow/tb-openalea": [
                "@vs-flow/tb-openalea_APIv01"
            ],
            "@vs-flow/tb-openalea/Doc": [
                "@vs-flow/tb-openalea_APIv01/Doc"
            ],
            "@w3nest/webpm-client": [
                "@w3nest/webpm-client_APIv01"
            ],
            "mkdocs-ts": [
                "mkdocs-ts_APIv05"
            ],
            "rx-vdom": [
                "rx-vdom_APIv01"
            ],
            "rxjs": [
                "rxjs_APIv7"
            ]
        }
    }
}
